{
  "name": "express-generator-ts",
  "version": "0.0.0",
  "scripts": {
    "dev": "nodemon --exec \"NODE_ENV=development ts-node ./src\" --watch ./src --ext .ts",
    "start": "node -r ./build/env/config.js ./build/index.js",
    "build": "ts-node ./scripts/build.ts",
    "lint": "eslint --flag unstable_ts_config",
    "type-check": "tsc --noEmit",
    "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && npm i"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^8.0.0",
    "inserturlparams": "^2.0.5",
    "jet-env": "^1.1.4",
    "jet-logger": "^2.0.1",
    "jet-paths": "^1.1.0",
    "jet-schema": "^1.4.3",
    "jet-validators": "^1.0.6",
    "jsonfile": "^6.1.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@stylistic/eslint-plugin-js": "^2.12.0",
    "@stylistic/eslint-plugin-ts": "^2.12.0",
    "@swc/core": "^1.10.1",
    "@types/cookie-parser": "^1.4.8",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jasmine": "^5.1.5",
    "@types/jsonfile": "^6.1.4",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.16.0",
    "eslint-plugin-n": "^17.14.0",
    "find": "^0.3.0",
    "fs-extra": "^11.2.0",
    "jasmine": "^5.5.0",
    "jiti": "^2.4.1",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0"
  }
}
